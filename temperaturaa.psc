Algoritmo temperaturaa
//Definir tipo de variable
	Definir temp Como Real;
//Asignar dato a la variable
	Escribir " Ingrese la temperatura actual";
	Leer temp ;
//Utilizar condicional Si y mostrar resultados
	si temp<=18 y temp>=25 Entonces
		Mostrar "La temperatura no es adecuada";
	sino 
		Mostrar "La temperatura es adecuada ";
	FinSi
FinAlgoritmo

